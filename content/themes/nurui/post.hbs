{{!< default}}
{{#post}}
<article>
	{{>posts}}
	<div class="section-post wrap">
		<div class="post-wrap {{^if feature_image}}no-image{{/if}}{{^if access}} no-access{{/if}}">
			{{#if access}}
			{{content}}
			{{else}}
			{{>members_cta}}
			{{/if}}
		</div>
		{{#if access}}
		{{>post_authors}}
		{{/if}}
		<div class="post-meta">
			{{>post_share}}
		</div>
	</div>
</article>
{{>post_navigation}}
<div class="section-disqus">
	{{#if access}}
	{{>services/disqus}}
	{{/if}}
</div>
{{#has tag="#subscribe-form"}}
{{>members_subscribe_form}}
{{/has}}
{{/post}}